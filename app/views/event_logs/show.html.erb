<%- model_class = EventLog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:event_type) %>:</strong></dt>
  <dd><%= @event_log.event_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:record_type) %>:</strong></dt>
  <dd><%= @event_log.record_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:record_id) %>:</strong></dt>
  <dd><%= @event_log.record_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:initiator_type) %>:</strong></dt>
  <dd><%= @event_log.initiator_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:initiator_id) %>:</strong></dt>
  <dd><%= @event_log.initiator_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:family1_id) %>:</strong></dt>
  <dd><%= @event_log.family1_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:family2_id) %>:</strong></dt>
  <dd><%= @event_log.family2_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:parent1_id) %>:</strong></dt>
  <dd><%= @event_log.parent1_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:parent2_id) %>:</strong></dt>
  <dd><%= @event_log.parent2_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:child1_id) %>:</strong></dt>
  <dd><%= @event_log.child1_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:child2_id) %>:</strong></dt>
  <dd><%= @event_log.child2_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:details) %>:</strong></dt>
  <dd><%= @event_log.details.inspect %></dd>
  <dt><strong><%= model_class.human_attribute_name(:created_at) %>:</strong></dt>
  <dd><%=l_with_time_zone @event_log.created_at %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              event_logs_path, :class => 'btn'  %>
</div>
